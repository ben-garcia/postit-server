  "scripts": {
    "db:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate",
    "db:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "db:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "db:show": "ts-node ./node_modules/typeorm/cli.js migration:show",
    "format": "prettier --write 'src/**/*.{ts,json,md}'",
    "start": "nodemon --exec ts-node src/index.ts",
    "start:cy": "CYPRESS_TEST=true nodemon --exec ts-node src/index.ts",
    "lint": "eslint . --ext js,ts --fix",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch  --runInBand --detectOpenHandles",
    "test:coverage": "jest --runInBand  --collectCoverage",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit"
  },
  "keywords": [
    "apollo-server",
    "github",
    "graphql",
    "typescript",
    "typeorm"
